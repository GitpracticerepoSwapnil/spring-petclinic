pipeline {
agent  any
    stages {
        stage('vcs') {
            steps {
                git branch: 'main' ,url: 'https://github.com/GitpracticerepoSwapnil/spring-petclinic.git'
            }
        }
        stage('build') {
            steps {
                sh '/usr/share/maven/bin/mvn package'
            }
        stage('archive') {
            steps {
                junit '**/surefire-reports/*.xml'
            }        
    }
}